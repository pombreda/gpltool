####################
### prerun scans ###
####################

[checkXML]
type    = prerun
module  = bat.prerun
method  = searchXML
priority = 100

[verifytext]
type    = prerun
module  = bat.prerun
method  = verifyText
priority = 1

[verifygraphics]
type    = prerun
module  = bat.prerun
method  = verifyGraphics
priority = 1

